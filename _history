mutation><value name=\"IF0\"><shadow type=\"logic_boolean\"><field name=\"BOOL\">TRUE</field></shadow><block type=\"logic_negate\"><value name=\"BOOL\"><shadow type=\"logic_boolean\"><field name=\"BOOL\">TRUE</field></shadow><block type=\"variables_get\"><field name=\"VAR\" id=\"$31|GjlLJp(w,/b8sbCN\">temBolsaVermelha</field></block></value></block></value><statement name=\"DO0\"><block type=\"variables_set\"><field name=\"VAR\" id=\"$31|GjlLJp(w,/b8sbCN\">temBolsaVermelha</field><value name=\"VALUE\"><shadow type=\"math_number\"><field name=\"NUM\">0</field></shadow><block type=\"logic_boolean\"><field name=\"BOOL\">TRUE</field></block></value><next><block type=\"story_show_text\"><mutation xmlns=\"http://www.w3.org/1999/xhtml\" _expanded=\"2\" _input_init=\"true\"></mutation><field name=\"speed\">story.TextSpeed.VerySlow</field><value name=\"text\"><shadow type=\"text\"><field name=\"TEXT\">Você volta ao quarto e acha uma pequena bolsa vermelha ao lado da cama. Você a pega.</field></shadow></value><value name=\"x\"><shadow type=\"math_number\"><field name=\"NUM\">80</field></shadow></value><value name=\"y\"><shadow type=\"math_number\"><field name=\"NUM\">140</field></shadow></value><value name=\"foreground\"><shadow type=\"colorindexpicker\"><field name=\"index\">1</field></shadow></value><value name=\"background\"><shadow type=\"colorindexpicker\"><field name=\"index\">15</field></shadow></value></block></next></block></statement><statement name=\"ELSE\"><block type=\"story_show_text\"><mutation xmlns=\"http://www.w3.org/1999/xhtml\" _expanded=\"2\" _input_init=\"true\"></mutation><field name=\"speed\">story.TextSpeed.VerySlow</field><value name=\"text\"><shadow type=\"text\"><field name=\"TEXT\">Você não encontra mais nada no quarto.</field></shadow></value><value name=\"x\"><shadow type=\"math_number\"><field name=\"NUM\">80</field></shadow></value><value name=\"y\"><shadow type=\"math_number\"><field name=\"NUM\">140</field></shadow></value><value name=\"fore